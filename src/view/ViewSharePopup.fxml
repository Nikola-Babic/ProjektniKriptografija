<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="450.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ViewShareController">
   <children>
      <ListView fx:id="shareList" layoutX="24.0" layoutY="22.0" prefHeight="367.0" prefWidth="403.0" />
      <Button fx:id="openFileBtn" layoutX="281.0" layoutY="405.0" mnemonicParsing="false" onAction="#openFile" prefHeight="25.0" prefWidth="68.0" text="OPEN" />
      <TextField fx:id="fileOpen" layoutX="83.0" layoutY="405.0" prefHeight="25.0" prefWidth="190.0" promptText="Name of file to open" />
      <Text layoutX="24.0" layoutY="422.0" strokeType="OUTSIDE" strokeWidth="0.0" text="File Name:" />
      <Button fx:id="showBtn" layoutX="359.0" layoutY="405.0" mnemonicParsing="false" onAction="#showShareContent" prefHeight="25.0" prefWidth="68.0" text="SHOW" />
   </children>
</AnchorPane>
